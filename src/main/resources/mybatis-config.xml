<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <properties>
        <property name="dialect" value="oracle"/>
    </properties>
    <settings>
        <setting name="cacheEnabled" value="false"/>
        <setting name="lazyLoadingEnabled" value="true"/>
        <setting name="aggressiveLazyLoading" value="false"/>
    </settings>
    <typeAliases>
        <typeAlias type="cn.kyle.shoppingMall.domain.BaseEntity" alias="BaseEntity"/>
        <typeAlias type="cn.kyle.shoppingMall.domain.Product" alias="Product"/>
        <typeAlias type="cn.kyle.shoppingMall.domain.ProductAndPageRelation" alias="ProductAndPageRelation"/>
        <typeAlias type="cn.kyle.shoppingMall.domain.ProductDetail" alias="ProductDetail"/>
        <typeAlias type="cn.kyle.shoppingMall.domain.ProductDetailInfo" alias="ProductDetailInfo"/>
        <typeAlias type="cn.kyle.shoppingMall.domain.ProductDetailInfoMsg" alias="ProductDetailInfoMsg"/>
        <typeAlias type="cn.kyle.shoppingMall.domain.ProductPage" alias="ProductPage"/>
        <typeAlias type="cn.kyle.shoppingMall.domain.ShoppingCart" alias="ShoppingCart"/>
        <typeAlias type="cn.kyle.shoppingMall.domain.User" alias="User"/>
    </typeAliases>
</configuration>